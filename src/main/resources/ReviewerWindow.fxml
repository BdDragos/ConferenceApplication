<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.cell.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<AnchorPane prefHeight="449.0" prefWidth="611.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TableView fx:id="fileTable" prefHeight="449.0" prefWidth="417.0">
            <columns>
                <TableColumn fx:id="topicC" prefWidth="75.0" text="Topic">
                    <cellValueFactory><PropertyValueFactory property="topic" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="keyC" prefWidth="75.0" text="Keywords">
                    <cellValueFactory><PropertyValueFactory property="keywords" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="docC" prefWidth="75.0" text="FileDoc">
                    <cellValueFactory><PropertyValueFactory property="filedoc" />
                    </cellValueFactory>
                </TableColumn>

                <TableColumn fx:id="acceptC" prefWidth="100.0" text="CurrentAccept">
                    <cellValueFactory><PropertyValueFactory property="level" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="norC" prefWidth="92.0" text="No. of Reviews">
                    <cellValueFactory><PropertyValueFactory property="reviewCount" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
        <ComboBox fx:id="acceptCB" layoutX="439.0" layoutY="273.0" prefWidth="150.0" />
        <Button fx:id="openFileBtn" layoutX="480.0" layoutY="72.0" mnemonicParsing="false" text="Open File" />
        <Label layoutX="483.0" layoutY="246.0" text="Accept Level" />
        <Button fx:id="reviewButton" layoutX="485.0" layoutY="314.0" mnemonicParsing="false" text="Review!" />
        <Button fx:id="logoutBtn" layoutX="487.0" layoutY="386.0" mnemonicParsing="false" text="Logout" />
    </children>
</AnchorPane>
